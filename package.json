{
  "name": "fps",
  "version": "1.0.0",
  "description": "A simple ray-casted old school fps ",
  "main": "src/main.js",
  "scripts": {
    "build": "npx webpack --config webpack.prod.js",
    "watch": "npx webpack --config webpack.dev.js --watch",
    "serve": "npx webpack-dev-server --open --config webpack.dev.js",
    "start": "npm run build && npx electron ."
